package logicalprogramming;

public class SortStringArray {
	public static void main(String[] args) {
		String str="java";
		char ch[]=str.toCharArray();
		char temp;
		for (int i = 0; i < ch.length; i++) {
			for (int j = i+1; j < ch.length; j++) {
				if(ch[i]>ch[j]) {
					temp=ch[i];
					ch[j]=ch[i];
					ch[i]=temp;
					
					
				}
				
			}
		}
		System.out.println(new String(ch));
	}

}
